<IfModule mod_rewrite.c>
    Options +FollowSymlinks
    RewriteEngine On

    # Explicitly disable rewriting for front controllers
    RewriteRule ^/tube/public/app_dev.php - [L]
    RewriteRule ^/tube/public/app.php - [L]

    # Fix the bundles folder
    RewriteRule ^bundles/(.*)$ /tube/public/bundles/$1  [QSA,L]

    RewriteCond %{REQUEST_FILENAME} !-f
    # Change below before deploying to production
    RewriteRule ^(.*)$ /tube/public/app.php [QSA,L]
    #RewriteRule ^(.*)$ /tube/public/app_dev.php [QSA,L]
</IfModule>